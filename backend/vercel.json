{
  "version": 2,
  "builds": [
    { "src": "stitch.py", "use": "@vercel/python" }
  ],
  "routes": [
    { "src": "/stitch/?", "dest": "stitch.py" },
    { "src": "/outputs/(.*)", "dest": "outputs/$1" }
  ],
  "functions": {
    "stitch.py": {
      "memory": 1024,
      "maxDuration": 30
    }
  }
}
